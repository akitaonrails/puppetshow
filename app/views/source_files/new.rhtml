<h1>New source_file</h1>

<%= error_messages_for :source_file %>

<% form_for(:source_file, :url => source_files_path) do |f| %>
  <p>
    <b>Filename</b><br />
    <%= f.text_field :filename %>
  </p>

  <p>
    <b>Path</b><br />
    <%= f.text_field :path %>
  </p>

  <p>
    <b>Updated at</b><br />
    <%= f.datetime_select :updated_at %>
  </p>

  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', source_files_path %>